        <Label text="Name" GridPane.halignment="CENTER"/>
        <Label text="Description" GridPane.halignment="CENTER" GridPane.rowIndex="1"/>
        <TextField fx:id="systemName" promptText="unique" GridPane.columnIndex="1"/>
        <TextArea fx:id="systemDescription" promptText="optional" GridPane.columnIndex="1"
